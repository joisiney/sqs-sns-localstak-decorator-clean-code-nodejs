{
  "name": "localstack-sns-sql",
  "version": "1.0.0",
  "description": "Create a SNS topic and subscribe a SQS queue to it.",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc && tsc-alias",
    "start:dev": "tsx watch src/server.ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage",
    "lint": "eslint --fix"
  },
  "author": "Joisiney Leandro",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sns": "^3.496.0",
    "@aws-sdk/client-sqs": "^3.496.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fastify": "^4.25.2",
    "reflect-metadata": "^0.2.1",
    "stacktrace-js": "^2.0.2",
    "useragent": "^2.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/useragent": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
